{
  "name": "beyondrounds",
  "version": "1.0.0",
  "description": "BeyondRounds - Medical Professional Matching Platform",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=tests/(components|hooks|lib)",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:api": "jest --testPathPattern=tests/api",
    "test:database": "jest --testPathPattern=tests/database",
    "test:performance": "jest --testPathPattern=tests/performance",
    "test:comprehensive": "jest --testPathPattern=tests/comprehensive",
    "test:system": "jest tests/simple-system-test.test.tsx --verbose",
    "test:system:watch": "jest tests/simple-system-test.test.tsx --watch",
    "test:system:coverage": "jest tests/simple-system-test.test.tsx --verbose --coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:api && npm run test:database",
    "test:full-suite": "npm run test:all && npm run test:performance && npm run test:comprehensive",
    "test:system-runner": "node scripts/run-system-tests.js",
    "test:system-quick": "node scripts/run-system-tests.js --quick",
    "test:coverage:unit": "jest --coverage --testPathPattern=tests/(components|hooks|lib)",
    "test:coverage:integration": "jest --coverage --testPathPattern=tests/integration",
    "test:coverage:api": "jest --coverage --testPathPattern=tests/api",
    "test:coverage:database": "jest --coverage --testPathPattern=tests/database",
    "test:coverage:comprehensive": "jest --coverage --testPathPattern=tests/comprehensive",
    "test:coverage:all": "jest --coverage --testPathPattern=tests/(components|hooks|lib|integration|api|database|comprehensive)",
    "test:runner": "node scripts/test-runner.js",
    "test:runner:all": "node scripts/test-runner.js all",
    "test:runner:coverage": "node scripts/test-runner.js all --coverage",
    "test:all-systems": "node scripts/run-all-tests.js",
    "type-check": "tsc --noEmit",
    "db:migrate": "echo 'Run migrations manually with psql'",
    "db:reset": "echo 'Reset database manually'",
    "db:seed": "echo 'Seed database manually'",
    "diagnose:auth": "node scripts/diagnostic-user-creation.js",
    "test:auth": "node scripts/quick-test.js",
    "test:login": "node scripts/test-login-functionality.js",
    "test:rls": "node scripts/test-rls-status.js",
    "check:schema": "node scripts/check-schema.js",
    "check:empty": "node scripts/check-empty-tables.js",
    "migrate-types": "node scripts/migrate-typescript-types.js",
    "cleanup-types": "npm run migrate-types && npm run type-check",
    "test:systems": "node scripts/test-systems.js",
    "test:verification": "node scripts/test-systems.js --verification-only",
    "test:matching": "node scripts/test-systems.js --matching-only",
    "test:quick": "npm run test:system",
    "test:complete": "npm run test:all-systems"
  },
